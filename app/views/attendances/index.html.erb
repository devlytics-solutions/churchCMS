<h1>Attendances</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Member</th>
    </tr>
  </thead>
  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.date %></td>
        <td><%= attendance.member.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
